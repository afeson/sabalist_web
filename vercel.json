{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/favicon(-\\d+)?.png",
      "dest": "/favicon$1.png"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/pwa/(.*)",
      "dest": "/pwa/$1"
    },
    {
      "src": "/delete-account",
      "dest": "/delete-account.html"
    },
    {
      "src": "/firestore-test.html",
      "dest": "/firestore-test.html"
    },
    {
      "src": "/app-firebase-test.html",
      "dest": "/app-firebase-test.html"
    },
    {
      "src": "/test-firestore.html",
      "dest": "/test-firestore.html"
    },
    {
      "src": "/check-auth.html",
      "dest": "/check-auth.html"
    },
    {
      "src": "/direct-firestore-write-test.html",
      "dest": "/direct-firestore-write-test.html"
    },
    {
      "src": "/simple-write-test.html",
      "dest": "/simple-write-test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload",
        "Content-Security-Policy": "upgrade-insecure-requests",
        "Referrer-Policy": "strict-origin-when-cross-origin"
      }
    }
  ]
}
